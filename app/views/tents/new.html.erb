<div class="container">
  <div class="row justify-content-center">
    <div class="col-md-8">
      <h1>Post my listing</h1>
      <%= simple_form_for @tent do |f| %>
        <%= f.input :title %>
        <%= f.input :description %>
        <%= f.input :address %>
        <%= f.input :photo %>
        <%= f.input :photo_cache, as: :hidden %>
        <%= f.input :price %>
        <%= f.button :submit, class: 'btn btn-primary' %>
      <% end %>
    </div>
  </div>
</div>
